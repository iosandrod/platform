{
  "name": "plaform",
  "description": "appPlaform",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.10.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "yarn",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon --exec \"node --inspect -r ts-node/register\" src/main_index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:app": "cross-env NODE_ENV=development ts-node ./appDbScript.ts",
    "migrate:main": "cross-env NODE_ENV=main ts-node ./dbScript.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@feathersjs/adapter-commons": "5.0.33",
    "@feathersjs/authentication": "5.0.33",
    "@feathersjs/authentication-client": "5.0.33",
    "@feathersjs/authentication-local": "^5.0.33",
    "@feathersjs/configuration": "5.0.33",
    "@feathersjs/errors": "5.0.33",
    "@feathersjs/feathers": "5.0.33",
    "@feathersjs/knex": "5.0.33",
    "@feathersjs/koa": "5.0.33",
    "@feathersjs/schema": "5.0.33",
    "@feathersjs/socketio": "5.0.33",
    "@feathersjs/transport-commons": "5.0.33",
    "@feathersjs/typebox": "5.0.33",
    "@scaleleap/pg-format": "^1.0.0",
    "@types/lodash": "^4.17.16",
    "ajv": "^8.17.1",
    "feathers-casl": "^2.2.0",
    "feathers-hooks-common": "^8.2.1",
    "ioredis": "^5.6.1",
    "knex": "3.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "pg": "8.13.3",
    "pg-format": "^1.0.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.33",
    "@feathersjs/rest-client": "5.0.33",
    "@types/mocha": "10.0.10",
    "@types/node": "22.13.5",
    "axios": "1.7.9",
    "cross-env": "^7.0.3",
    "mocha": "11.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "shx": "0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  }
}
